	<key>command</key>
	<string>#!/usr/bin/env perl

BEGIN {our $DEBUG=0;push(@INC,"$ENV{TM_BUNDLE_SUPPORT}/lib")};

require "smartReturn.pl";

process(
	[	# pairs
		['{', '}'],
		['(', ')'],
		['\\bif\\b', '\\bfi\\b', 'fi'],
		['\\bcase\\b', '\\besac\\b', 'esac'],
	],
	[	# string pairs
		['"', '"'],
		["'", "'"],
	],
	1	# Extend single-line comments
);
</string>
